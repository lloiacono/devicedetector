{
  "name": "TestApp",
  "description": "Test App",
  "type": "project",
  "require": {
    "php": ">=7",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "mobiledetect/mobiledetectlib": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
